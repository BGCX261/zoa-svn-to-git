 <method CLSNAME="ZOA__SUPERCLASS" CMPNAME="BUILD_SET_EXPRESSION" VERSION="1" LANGU="E" DESCRIPT="Create an expression string for update from field values" EXPOSURE="1" STATE="1" EDITORDER="25 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="BUILD_SET_EXPRESSION" SCONAME="SAP_TABLE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="BUILD_SET_EXPRESSION" SCONAME="FIELD_VALUES" VERSION="1" LANGU="E" DESCRIPT="Field values table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZOA__FIELD_VALUES"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="BUILD_SET_EXPRESSION" SCONAME="EXPRESSION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <source>method BUILD_SET_EXPRESSION.
*/---------------------------------------------------------------------\
*|   This file is part of the ZOA Integration Framework.                                     |
*|                                                                     |
*|   ZOA is free software; you can redistribute it and/or modify       |
*|   it under the terms of the GNU General Public License as published |
*|   by the Free Software Foundation; either version 2 of the License, |
*|   or (at your option) any later version.                            |
*|                                                                     |
*|   ZOA is distributed in the hope that it will be useful,            |
*|   but WITHOUT ANY WARRANTY; without even the implied warranty of    |
*|   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the     |
*|   GNU General Public License for more details.                      |
*|                                                                     |
*|   You should have received a copy of the GNU General Public License |
*|   along with SAPlink; if not, write to the                          |
*|   Free Software Foundation, Inc.,                                   |
*|   51 Franklin St, Fifth Floor, Boston, MA  02110-1301  USA          |
*\---------------------------------------------------------------------/



  field-symbols:
     &lt;f&gt;                     type zoa__field_value.

  data:
    lv_value_text            type string,
    lv_sap_table             type string,
    lv_sap_field             type string.

  loop at field_values assigning &lt;f&gt;.

    get_field(
      exporting
        field_name           = &lt;f&gt;-field_name
       importing
         sap_table           = lv_sap_table
         sap_field           = lv_sap_field ).

    check lv_sap_table = sap_table.


    concatenate &apos;&apos;&apos;&apos; &lt;f&gt;-value &apos;&apos;&apos;&apos; into lv_value_text.

    concatenate expression lv_sap_field &apos;=&apos; lv_value_text into expression
      separated by space.

  endloop.

endmethod.</source>
 </method>
