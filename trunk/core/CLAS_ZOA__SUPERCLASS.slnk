 <publicSection>class ZOA__SUPERCLASS definition
  public
  create public .

*&quot;* public components of class ZOA__SUPERCLASS
*&quot;* do not include other source files here!!!
public section.

  types:
    begin of message_type,
        text(255) type c,
        id(80) type c,
        severity(1) type c,
        extensions type zoa__field_values,
      end of message_type .
  types:
    messages_type type table of message_type .
  types:
    begin of change_buffer_type,
        temporary_id           type zoa__id,
        permanent_id           type zoa__id,
      end of change_buffer_type .
  types:
    change_buffers_type    type table of change_buffer_type .

  constants MESSAGE_WARNING type CHAR1 value 1. &quot;#EC NOTEXT
  constants MESSAGE_INFO type CHAR1 value 0. &quot;#EC NOTEXT
  constants MESSAGE_CRITICAL type CHAR1 value 2. &quot;#EC NOTEXT
  constants CHILD_RELATIONSHIP type STRING value &apos;Child&apos;. &quot;#EC NOTEXT
  data RELATED_FEATURES type ZOA__RELATED_FEATURES .
  data ERROR_FLAG type CHAR1 read-only .
  data MESSAGES type ZOA__MESSAGES read-only .
  data RESULTS type ZOA__RESULT_CELLS read-only .
  data FILTERS type STRING read-only .
  data FIELDS type ZOA__FIELDS read-only .
  data EXTENSIONS type ZOA__FIELD_VALUES read-only .

  methods ADD_EXTENSION_FIELD
    importing
      value(FIELD_NAME) type STRING
      value(VALUE) type STRING .
  methods ADD_FILTER
    importing
      value(FIELD_NAME) type STRING
      value(EXACT_MATCH) type STRING optional
      value(BEGINS_WITH) type STRING optional
      value(CONTAINS) type STRING optional
      value(GREATER_THAN) type STRING optional
      value(LESS_THAN) type STRING optional
      value(GREATER_THAN_OR_EQUAL) type STRING optional
      value(LESS_THAN_OR_EQUAL) type STRING optional
      value(EXCLUDE) type STRING optional
      value(INTERPRET) type STRING optional
    raising
      ZCX_ZOA_DEV .
  methods CONSTRUCTOR .
  methods FILL_MESSAGES
    exporting
      !MESSAGE_TABLE type ANY .
  methods FILL_RESULTS
    exporting
      !RESULTS_TABLE type TABLE .
  methods FILL_RESULT_EXTENSIONS
    exporting
      !RESULT_EXTENSIONS type ZOA__INDEXED_FIELD_VALUES .
  methods GET_DESCRIPTION
    exporting
      !ID type ZOA__ID
      !NAME type STRING
      !LONG_NAME type STRING
      !DESCRIPTION type STRING
      !APPLICATION_AREA type STRING
      !VERSION type STRING .
  methods IS_FIELD_VALUE_VALID
    importing
      !FIELD_NAME type STRING
      !VALUE type STRING
    exporting
      !ERROR_FLAG type CHAR1 .
  methods IS_USER_AUTHORISED
    importing
      !ACTIVITY type STRING default &apos;READ&apos;
      !FIELD_NAME type STRING optional
      !EXTENSIONS type ZOA__FIELD_VALUES optional
      preferred parameter activity
    returning
      value(IS_AUTHORISED) type FLAG .
  methods LIST_ALLOWED_FIELD_VALUES
    importing
      !FIELD_NAME type STRING
      !RESTRICTIONS type ZOA__FIELD_VALUES optional
    exporting
      !RESULTS type ZOA__INDEXED_FIELD_VALUES .
  methods LIST_FIELDS .
  methods LIST_PROPOSED_VALUES .
  methods LIST_RELATED_FEATURES
    exporting
      !RELATED_FEATURES type ZOA__RELATED_FEATURES .
  methods LOG_MESSAGE
    importing
      !T1 type ANY
      !T2 type ANY optional
      !T3 type ANY optional
      !T4 type ANY optional
      !SEVERITY type C default &apos;2&apos; .
  methods REFRESH .
  methods SAP_CHANGE
    importing
      !ID type ZOA__ID
      value(FIELD_VALUES) type ZOA__FIELD_VALUES optional .
  methods SAP_COMMIT .
  methods SAP_CREATE
    importing
      !FIELD_VALUES type ZOA__FIELD_VALUES
    exporting
      !ID type ZOA__ID .
  methods SAP_LIST_IDS
    importing
      !MAX_ROWS type I default 100
      !SKIP_ROWS type I default 0
    exporting
      !ROW_COUNT type I
      !IDS type ZOA__IDS .
  methods SAP_READ
    importing
      !ID type ZOA__ID .
  methods SAP_SEARCH
    importing
      value(MAX_ROWS) type I default 100
      value(SKIP_ROWS) type I default 0
    exporting
      value(ROW_COUNT) type I .
  methods SET_EXTENSIONS
    importing
      !EXTENSIONS type ZOA__FIELD_VALUES .</publicSection>
