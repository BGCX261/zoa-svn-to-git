 <method CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_EXTENSIONS" VERSION="1" LANGU="E" DESCRIPT="Transfer data from an internal SAP structure to results tab" EXPOSURE="1" STATE="1" EDITORDER="31 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_EXTENSIONS" SCONAME="STRUCTURE" VERSION="1" LANGU="E" DESCRIPT="SAP structure name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_EXTENSIONS" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="Data in structure" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ANY"/>
  <exception CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_EXTENSIONS" SCONAME="ZCX_ZOA_DEV" VERSION="1" LANGU="E" DESCRIPT="Exceptions for the developer to fix" MTDTYPE="0" EDITORDER="1 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000"/>
  <source>method MAP_TO_EXTENSIONS.
*/---------------------------------------------------------------------\
*|   This file is part of the ZOA Integration Framework.                                     |
*|                                                                     |
*|   ZOA is free software; you can redistribute it and/or modify       |
*|   it under the terms of the GNU General Public License as published |
*|   by the Free Software Foundation; either version 2 of the License, |
*|   or (at your option) any later version.                            |
*|                                                                     |
*|   ZOA is distributed in the hope that it will be useful,            |
*|   but WITHOUT ANY WARRANTY; without even the implied warranty of    |
*|   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the     |
*|   GNU General Public License for more details.                      |
*|                                                                     |
*|   You should have received a copy of the GNU General Public License |
*|   along with SAPlink; if not, write to the                          |
*|   Free Software Foundation, Inc.,                                   |
*|   51 Franklin St, Fifth Floor, Boston, MA  02110-1301  USA          |
*\---------------------------------------------------------------------/



  field-symbols:
    &lt;field&gt; type zoa__field,
    &lt;value&gt; type any,
    &lt;ext&gt; type zoa__field_value.

  clear extensions.

*-- Add a results row
  loop at fields assigning &lt;field&gt; where sap_table = structure.

    assign component &lt;field&gt;-sap_field of structure data to &lt;value&gt;.
    if sy-subrc &lt;&gt; 0.
      raise exception type zcx_zoa_dev
        exporting
          textid = zcx_zoa_dev=&gt;unknown_field.
    endif.

    check &lt;value&gt; is not initial.

    insert initial line into table extensions assigning &lt;ext&gt;.
    &lt;ext&gt;-field_name = &lt;field&gt;-field_name.
    &lt;ext&gt;-value = &lt;value&gt;.

  endloop.

endmethod.</source>
 </method>
 <method CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_RESULTS" VERSION="1" LANGU="E" DESCRIPT="Transfer data from an internal SAP structure to results tab" EXPOSURE="1" STATE="1" EDITORDER="32 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_RESULTS" SCONAME="STRUCTURE" VERSION="1" LANGU="E" DESCRIPT="SAP structure name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="MAP_TO_RESULTS" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="Data in structure" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ANY"/>
  <source>method MAP_TO_RESULTS.
*/---------------------------------------------------------------------\
*|   This file is part of the ZOA Integration Framework.                                     |
*|                                                                     |
*|   ZOA is free software; you can redistribute it and/or modify       |
*|   it under the terms of the GNU General Public License as published |
*|   by the Free Software Foundation; either version 2 of the License, |
*|   or (at your option) any later version.                            |
*|                                                                     |
*|   ZOA is distributed in the hope that it will be useful,            |
*|   but WITHOUT ANY WARRANTY; without even the implied warranty of    |
*|   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the     |
*|   GNU General Public License for more details.                      |
*|                                                                     |
*|   You should have received a copy of the GNU General Public License |
*|   along with SAPlink; if not, write to the                          |
*|   Free Software Foundation, Inc.,                                   |
*|   51 Franklin St, Fifth Floor, Boston, MA  02110-1301  USA          |
*\---------------------------------------------------------------------/



  field-symbols:
    &lt;field&gt; type zoa__field,
    &lt;value&gt; type any,
    &lt;result&gt; type zoa__result_cell.

*-- Handle selective results
  if result_fields is initial.
    result_fields = fields.
    result_row_number = 1.
  endif.

*-- Build header line
  if result_row_number = 0.
    loop at result_fields assigning &lt;field&gt; where sap_table = structure.
      insert initial line into table results assigning &lt;result&gt;.
      &lt;result&gt;-column_number = sy-tabix.
      &lt;result&gt;-row_number = 0.
      &lt;result&gt;-value = &lt;field&gt;-field_name.
    endloop.
    result_row_number = 1.
  endif.

*-- Add a results row
  loop at result_fields assigning &lt;field&gt; where sap_table = structure.

    assign component &lt;field&gt;-sap_field of structure data to &lt;value&gt;.
    check sy-subrc = 0 and &lt;value&gt; is not initial.

    insert initial line into table results assigning &lt;result&gt;.
    &lt;result&gt;-column_number = sy-tabix.
    &lt;result&gt;-row_number = result_row_number.
    &lt;result&gt;-value = &lt;value&gt;.

  endloop.

  result_row_number = result_row_number + 1.

endmethod.</source>
 </method>
