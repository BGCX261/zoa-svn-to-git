 <method CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" VERSION="1" LANGU="E" DESCRIPT="Register a field to be part of the feature" EXPOSURE="1" STATE="1" EDITORDER="33 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="FIELD_NAME" VERSION="1" LANGU="E" DESCRIPT="Friendly field name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="SAP_TABLE" VERSION="1" LANGU="E" DESCRIPT="Source SAP table" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PAROPTIONL="X"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="SAP_FIELD" VERSION="1" LANGU="E" DESCRIPT="Source SAP field, if different from FIELD" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PAROPTIONL="X"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="SEARCH_FIELD" VERSION="1" LANGU="E" DESCRIPT="Matchcode field for uppercase searches" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PAROPTIONL="X"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="DESCRIPTION" VERSION="1" LANGU="E" DESCRIPT="Description if different from DDIC" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PAROPTIONL="X"/>
  <parameter CLSNAME="ZOA__SUPERCLASS" CMPNAME="REGISTER_FIELD" SCONAME="IS_FILTER" VERSION="1" LANGU="E" DESCRIPT="Flag if used to filter search results" CMPTYPE="1" MTDTYPE="0" EDITORDER="6 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20090427" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PAROPTIONL="X"/>
  <source>method REGISTER_FIELD.
*/---------------------------------------------------------------------\
*|   This file is part of the ZOA Integration Framework.                                     |
*|                                                                     |
*|   ZOA is free software; you can redistribute it and/or modify       |
*|   it under the terms of the GNU General Public License as published |
*|   by the Free Software Foundation; either version 2 of the License, |
*|   or (at your option) any later version.                            |
*|                                                                     |
*|   ZOA is distributed in the hope that it will be useful,            |
*|   but WITHOUT ANY WARRANTY; without even the implied warranty of    |
*|   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the     |
*|   GNU General Public License for more details.                      |
*|                                                                     |
*|   You should have received a copy of the GNU General Public License |
*|   along with SAPlink; if not, write to the                          |
*|   Free Software Foundation, Inc.,                                   |
*|   51 Franklin St, Fifth Floor, Boston, MA  02110-1301  USA          |
*\---------------------------------------------------------------------/



  data:
    ls_field type zoa__field,
    lv_sap_table type dfies-tabname,
    lv_sap_field type dfies-lfieldname,
    lv_sap_description type dd04v-scrtext_l.

  ls_field-field_name        = field_name.
  translate ls_field-field_name to upper case.
  ls_field-field_name_lowercase    = field_name.
  ls_field-sap_table          = sap_table.
  ls_field-is_core            = is_core_field( ls_field-field_name ).
  ls_field-is_filter          = is_filter.
  if sap_field is initial.
    ls_field-sap_field        = field_name.
  else.
    ls_field-sap_field        = sap_field.
  endif.

  if search_field is initial.
    ls_field-search_field = ls_field-sap_field.
  else.
    ls_field-search_field = search_field.
    ls_field-is_filter    = abap_true.   &quot; force searchable
  endif.

  if description is supplied and description is not initial.
    ls_field-description  = description.
  else.
    lv_sap_table = sap_table.
    lv_sap_field = sap_field.

    call function &apos;DDIC_FIELDNAME_GET&apos;
      exporting
        iv_strucname         = lv_sap_table
        iv_fieldname         = lv_sap_field
      changing
        cv_field_description = lv_sap_description
      exceptions
        no_data              = 1
        others               = 2.

    if sy-subrc &lt;&gt; 0.
      log_message(
        exporting
          t1 = &apos;There was a problem registering field&apos;
          t2 = field_name
          severity = &apos;5&apos; ).
    endif.

    ls_field-description = lv_sap_description.

  endif.

  append ls_field to fields.

endmethod.</source>
 </method>
